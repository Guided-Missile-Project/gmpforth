<glossentry id="core-c-fetch" role="CORE" xreflabel="c@">
  <glossterm>
    <indexterm role="forth"><primary>c@</primary></indexterm>
    <cmdsynopsis>
      <command>c@</command>
      <group role="stack">
        <arg>addr</arg>
        <arg>--</arg>
        <arg>char</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Fetch <parameter>char</parameter> from the address
      <parameter>addr</parameter>.
    </para>
    <example>
      <title>c@</title>
      <screen>
<userinput>10 pad c! pad c@ .<keysym>&larrhk;</keysym></userinput> 10  ok
