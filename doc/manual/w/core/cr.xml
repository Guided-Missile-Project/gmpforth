<glossentry id="core-cr" role="CORE" xreflabel="cr">
  <glossterm>
    <indexterm role="forth"><primary>cr</primary></indexterm>
    <cmdsynopsis>
      <command>cr</command>
      <group role="stack">
        <arg>--</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Emit an <quote>end of line </quote> sequence such that
      subsequent output begins on a new line.
    </para>
    <example>
      <title>cr</title>
      <screen>
<userinput>." hello" cr ." there " cr<keysym>&larrhk;</keysym></userinput> hello
there 
 ok
      </screen>
    </example>
  </glossdef>
</glossentry>
