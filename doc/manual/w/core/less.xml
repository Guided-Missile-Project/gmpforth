<!DOCTYPE glossentry PUBLIC "-//OASIS//DTD DocBook XML V4.3//EN"
                  "http://www.oasis-open.org/docbook/xml/4.3/docbookx.dtd">
<glossentry id="core-less" role="CORE" xreflabel="&lt;">
  <glossterm>
    <indexterm role="forth"><primary>&lt;</primary></indexterm>
    <cmdsynopsis>
      <command>&lt;</command>
      <group role="stack">
        <arg>n1</arg>
        <arg>n2</arg>
        <arg>--</arg>
        <arg>flag</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Return <xref linkend="core-ext-true"/> if
      <parameter>n<subscript>1</subscript></parameter>
      is less than <parameter>n<subscript>2</subscript></parameter>,
      otherwise return <xref linkend="core-ext-false"/>.
    </para>
    <example>
      <title>&lt;</title>
      <screen>
<userinput>1 2 &lt; .<keysym>&larrhk;</keysym></userinput> -1  ok
<userinput>1 1 &lt; .<keysym>&larrhk;</keysym></userinput> 0  ok
<userinput>2 1 &lt; .<keysym>&larrhk;</keysym></userinput> 0  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
