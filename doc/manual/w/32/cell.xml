<glossentry id="thirty-two-cell" role="32" xreflabel="(cell)">
  <glossterm>
    <indexterm role="forth"><primary>(cell)</primary></indexterm>
    <cmdsynopsis>
      <command>(cell)</command>
      <group role="stack">
        <arg>--</arg>
        <arg>n</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      A constant that returns the number of 8 bit bytes in a cell.
    </para>
    <example>
      <title>(cell)</title>
      <screen>
<userinput>(cell) .<keysym>&larrhk;</keysym></userinput> 4  ok
      </screen>
    </example>
  </glossdef>
