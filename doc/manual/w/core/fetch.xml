<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE glossentry PUBLIC "-//OASIS//DTD DocBook XML V4.3//EN"
                  "http://www.oasis-open.org/docbook/xml/4.3/docbookx.dtd">
<glossentry id="core-fetch" role="CORE" xreflabel="@">
  <glossterm>
    <indexterm role="forth"><primary>@</primary></indexterm>
    <cmdsynopsis>
      <command>@</command>
      <group role="stack">
        <arg>a-addr</arg>
        <arg>--</arg>
        <arg>x</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Fetch <parameter>x</parameter> from the address
      <parameter>a-addr</parameter>. Some targets may act exceptionally
      if <parameter>a-addr</parameter> is not aligned.
    </para>
    <example>
      <title>@</title>
      <screen>
<userinput>base @ .<keysym>&larrhk;</keysym></userinput> 10  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
