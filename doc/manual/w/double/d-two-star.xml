<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE glossentry PUBLIC "-//OASIS//DTD DocBook XML V4.3//EN"
                  "http://www.oasis-open.org/docbook/xml/4.3/docbookx.dtd">
<glossentry id="double-two-star" role="DOUBLE" xreflabel="d2*">
  <glossterm>
    <indexterm role="forth"><primary>d2*</primary></indexterm>
    <cmdsynopsis>
      <command>d2*</command>
      <group role="stack">
        <arg>d1</arg>
        <arg>--</arg>
        <arg>d2</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Multiply double number
      <parameter>d<subscript>1</subscript></parameter> by 2 returning
      double number <parameter>d<subscript>2</subscript></parameter>.
    </para>
    <example>
      <title>d2*</title>
      <screen>
<userinput>100. 2* d.<keysym>&larrhk;</keysym></userinput> 200  ok
<userinput>-100. 2* d.<keysym>&larrhk;</keysym></userinput> -200  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
