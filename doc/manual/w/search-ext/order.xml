<glossentry id="search-ext-order" role="SEARCH-EXT" xreflabel="order">
  <glossterm>
    <indexterm role="forth"><primary>order</primary></indexterm>
    <cmdsynopsis>
      <command>order</command>
      <group role="stack">
        <arg>--</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Display the context search order and current wordlist. The
      context list is displayed first, in search order, followed by a
      slash, and then the current wordlist.
    </para>
    <example>
      <title>order</title>
      <screen>
<userinput>order<keysym>&larrhk;</keysym></userinput> forth / forth ok
      </screen>
    </example>
  </glossdef>
</glossentry>
