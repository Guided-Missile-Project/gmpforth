<glossentry id="core-to-r" role="CORE,C" xreflabel="&gt;r">
  <glossterm>
    <indexterm role="forth"><primary>&gt;r</primary></indexterm>
    <cmdsynopsis>
      <command>&gt;r</command>
      <group role="stack">
        <arg>x</arg>
        <arg>--</arg>
      </group>
      <group role="stack">
        <arg>R:</arg>
        <arg>--</arg>
        <arg>x</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Move <parameter>x</parameter> from the
      data stack to the return stack.
    </para>
    <example>
      <title>&gt;r</title>
      <screen>
<userinput>: test 1 2 3 &gt;r 1+ r&gt; .s ; test<keysym>&larrhk;</keysym></userinput> 1 3 3  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
