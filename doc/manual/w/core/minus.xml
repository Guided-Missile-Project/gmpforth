<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE glossentry PUBLIC "-//OASIS//DTD DocBook XML V4.3//EN"
                  "http://www.oasis-open.org/docbook/xml/4.3/docbookx.dtd">
<glossentry id="core-minus" role="CORE" xreflabel="-">
  <glossterm>
    <indexterm role="forth"><primary>-</primary></indexterm>
    <cmdsynopsis>
      <command>-</command>
      <group role="stack">
        <arg>n1</arg>
        <arg>n2</arg>
        <arg>--</arg>
        <arg>n3</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Subtract <parameter>n<subscript>2</subscript></parameter> from
      <parameter>n<subscript>1</subscript></parameter>, returning
      the difference <parameter>n<subscript>3</subscript></parameter>.
    </para>
    <example>
      <title>-</title>
      <screen>
<userinput>10 7 - .<keysym>&larrhk;</keysym></userinput> 3  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
