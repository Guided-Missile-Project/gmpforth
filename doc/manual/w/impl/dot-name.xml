<glossentry id="impl-dot-name" role="IMPL" xreflabel=".name">
  <glossterm>
    <indexterm role="forth"><primary>.name</primary></indexterm>
    <cmdsynopsis>
      <command>.name</command>
      <group role="stack">
        <arg>xt</arg>
        <arg>--</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Display the name of the word corresponding to <parameter>xt</parameter>.
    </para>
    <example>
      <title>.name</title>
      <screen>
<userinput>' cold .name<keysym>&larrhk;</keysym></userinput> cold ok
      </screen>
    </example>
  </glossdef>
</glossentry>
