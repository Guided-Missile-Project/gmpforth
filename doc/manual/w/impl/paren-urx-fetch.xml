<glossentry id="impl-paren-urx-fetch" role="IMPL" xreflabel="(urx@)">
  <glossterm>
    <indexterm role="forth"><primary>(urx@)</primary></indexterm>
    <cmdsynopsis>
      <command>(urx@)</command>
      <group role="stack">
        <arg>--</arg>
        <arg>c</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Return a character from the console. This will block if a
      character is not ready to be received.
    </para>
    <example>
      <title>(urx@)</title>
      <screen>
<userinput>(urx@) .<keysym>&larrhk;</keysym>A</userinput> 65  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
