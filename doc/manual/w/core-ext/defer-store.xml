<glossentry id="core-ext-defer-store" role="CORE-EXT" xreflabel="defer!">
  <glossterm>
    <indexterm role="forth"><primary>defer!</primary></indexterm>
    <cmdsynopsis>
      <command>defer!</command>
      <group role="stack">
        <arg>xt1</arg>
        <arg>xt2</arg>
        <arg>--</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
       Save <parameter>xt1</parameter> as the configured action of
       <parameter>xt2</parameter> created by <xref
       linkend="core-ext-defer"/>.
    </para>
    <example>
      <title>defer!</title>
      <screen>
<userinput>defer x<keysym>&larrhk;</keysym></userinput>  ok
<userinput>' + ' x defer!<keysym>&larrhk;</keysym></userinput>  ok
<userinput>1 1 x .<keysym>&larrhk;</keysym></userinput> 2  ok
      </screen>
    </example>
