<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE glossentry PUBLIC "-//OASIS//DTD DocBook XML V4.3//EN"
                  "http://www.oasis-open.org/docbook/xml/4.3/docbookx.dtd">
<glossentry id="core-store" role="CORE" xreflabel="!">
  <glossterm>
    <indexterm role="forth"><primary>!</primary></indexterm>
    <cmdsynopsis>
      <command>!</command>
      <group role="stack">
        <arg>x</arg>
        <arg>a-addr</arg>
        <arg>--</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Store <parameter>x</parameter> at the address
      <parameter>a-addr</parameter>. Some targets may act exceptionally
      if <parameter>a-addr</parameter> is not aligned.
    </para>
    <example>
      <title>!</title>
      <screen>
<userinput>10 base !<keysym>&larrhk;</keysym></userinput> ok
      </screen>
    </example>
  </glossdef>
</glossentry>
