<glossentry id="core-semicolon" role="CORE,IC" xreflabel=";">
  <glossterm>
    <indexterm role="forth"><primary>;</primary></indexterm>
    <cmdsynopsis>
      <command>;</command>
      <group role="stack">
        <arg>--</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Terminate a colon (<xref linkend="core-colon"/>) definition and enter the
      interpretation state.
    </para>
    <example>
      <title>;</title>
      <screen>
<userinput>: test 1 1 + . ;<keysym>&larrhk;</keysym></userinput>  ok
<userinput>test<keysym>&larrhk;</keysym></userinput> 2  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
