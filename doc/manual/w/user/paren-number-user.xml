<glossentry id="user-paren-number-user" role="USER" xreflabel="(#user)">
  <glossterm>
    <indexterm role="forth"><primary>(#user)</primary></indexterm>
    <cmdsynopsis>
      <command>(#user)</command>
      <group role="stack">
        <arg>--</arg>
        <arg>a-addr</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
