<glossentry id="core-dup" role="CORE" xreflabel="dup">
  <glossterm>
    <indexterm role="forth"><primary>dup</primary></indexterm>
    <cmdsynopsis>
      <command>dup</command>
      <group role="stack">
        <arg>x</arg>
        <arg>--</arg>
        <arg>x</arg>
        <arg>x</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Duplicate the top itemon the stack.
    </para>
    <example>
      <title>dup</title>
      <screen>
<userinput>1 dup .s<keysym>&larrhk;</keysym></userinput> 1 1  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
