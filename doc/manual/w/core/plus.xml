<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE glossentry PUBLIC "-//OASIS//DTD DocBook XML V4.3//EN"
                  "http://www.oasis-open.org/docbook/xml/4.3/docbookx.dtd">
<glossentry id="core-plus" role="CORE" xreflabel="+">
  <glossterm>
    <indexterm role="forth"><primary>+</primary></indexterm>
    <cmdsynopsis>
      <command>+</command>
      <group role="stack">
        <arg>n1</arg>
        <arg>n2</arg>
        <arg>--</arg>
        <arg>n3</arg>
      </group>
    </cmdsynopsis>
  </glossterm>
  <glossdef>
    <para>
      Add <parameter>n<subscript>1</subscript></parameter> to
      <parameter>n<subscript>2</subscript></parameter>, returning
      the sum <parameter>n<subscript>3</subscript></parameter>.
    </para>
    <example>
      <title>+</title>
      <screen>
<userinput>1 1 + .<keysym>&larrhk;</keysym></userinput> 2  ok
      </screen>
    </example>
  </glossdef>
</glossentry>
